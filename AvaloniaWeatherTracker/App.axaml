<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AvaloniaWeatherTracker"
             x:Class="AvaloniaWeatherTracker.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
        <StyleInclude Source="Styles/ExtendedPropertyStyle.axaml"/>
        <StyleInclude Source="Styles/ExtendedSeparatorStyle.axaml"/>
        <StyleInclude Source="Styles/ExtendedImageStyle.axaml"/>
        <StyleInclude Source="Styles/WeeklyImageStyle.axaml"/>
        <StyleInclude Source="Styles/WeeklyDateStyle.axaml"/>
        <StyleInclude Source="Styles/WeeklyDegreeStyle.axaml"/>
    </Application.Styles>
             
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
               
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>